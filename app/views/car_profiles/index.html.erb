<p id="notice"><%= notice %></p>

<h1>Listing Car Profiles</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @car_profiles.each do |car_profile| %>
      <tr>
        <td><%= link_to 'Show', car_profile %></td>
        <td><%= link_to 'Edit', edit_car_profile_path(car_profile) %></td>
        <td><%= link_to 'Destroy', car_profile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Car profile', new_car_profile_path %>
